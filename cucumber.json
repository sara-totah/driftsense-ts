{
    "default": {
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "paths": [
            "src/tests/**/*.feature"
        ],
        "dryRun": false,
        "require": [
            "src/tests/**/*.ts",
            "src/hooks/hooks.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "progress-bar",
            "html:test-results/cucumber-report.html",
            "json:test-results/cucumber-report.json",
            "rerun:test-results/@rerun.txt"
        ],
        "parallel": 3
    },
    "rerun": {
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "paths": [
            "src/tests/**/*.feature"
        ],
        "dryRun": false,
        "require": [
            "src/tests/**/*.ts",
            "src/hooks/hooks.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "progress-bar",
            "html:test-results/cucumber-report.html",
            "json:test-results/cucumber-report.json",
            "rerun:test-results/@rerun.txt"
        ],
        "parallel": 3
    }
}